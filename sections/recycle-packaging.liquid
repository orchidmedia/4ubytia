<!-- Recycle & Packaging starts -->
{% assign ss = section.settings %}
{% assign i = 0 %}
<div style="background:{{ss.pack_bg_color}}">
  <div class="recycle-Packaging-main {{ss.recycle_container}}" 
    style="padding:{{ss.pack_dp_top}}px {{ss.pack_dp_right}}px {{ss.pack_dp_bottom}}px {{ss.pack_dp_left}}px;"
  >
    <div class="recycle-Packaging-content">
      <div class="recycle-Packaging-Heading flex"
        style="justify-content:{{ss.main_heading_alignment}};align-items:{{ss.main_heading_alignment}};
          padding:{{ss.main_heading_top}}px {{ss.main_heading_right}}px {{ss.main_heading_bottom}}px {{ss.main_heading_left}}px;"
      >
        {%- capture heading_color -%}
            color: {{ss.main_title_color}};
        {%- endcapture -%}
        {% render 'heading-tag' 
            heading: ss.pack_main_title_tag,
            content: ss.pack_main_title,
            custom_styles: heading_color
        %}
      </div>
      <div class="recycle-Packaging-desc flex" 
        style="justify-content:{{ss.main_desc_alignment}};align-items:{{ss.main_desc_alignment}};
          padding:{{ss.main_desc_dp_top}}px {{ss.main_desc_dp_right}}px {{ss.main_desc_dp_bottom}}px {{ss.main_desc_dp_left}}px;"
      >
        {%- capture desc_color -%}
            color: {{ss.main_desc_color}};
        {%- endcapture -%}
        {% render 'heading-tag' 
            heading: ss.pack_main_content_tag,
            content: ss.pack_main_content,
            custom_styles: desc_color
        %}
      </div>
        
      {% for block in section.blocks %}
        {% assign bs = block.settings %}
        <div class="recycle-Packaging-repeating-content recycle-Packaging-repeating-desktop flex" data-section-id="{{ block.id }}"
          style="margin:{{bs.repeat_sec_m_top}}px {{bs.repeat_sec_m_right}}px {{bs.repeat_sec_m_bottom}}px {{bs.repeat_sec_m_left}}px;"
        >
          <div class="recycle-Packaging-circle">
            <span>{{ forloop.index }}</span>
          </div>
          <div class="recycle-Packaging-repeating-desc flex">
            <h1
              style="color:{{bs.repeat_title_color}};font-size:{{bs.repeat_heading_font_size}}px;font-weight:{{bs.repeat_heading_font_weight}};text-align:{{bs.repeat_heading_alignment}};
              padding:{{bs.repeat_heading_top}}px {{bs.repeat_heading_right}}px {{bs.repeat_heading_bottom}}px {{bs.repeat_heading_left}}px;"
            >{{bs.repeat_sec_title}}</h1>
            <p
              style="color:{{bs.repeat_desc_color}};font-size:{{bs.repeat_desc_font_size}}px;font-weight:{{bs.repeat_desc_font_weight}};text-align:{{bs.repeat_desc_alignment}};
              padding:{{bs.repeat_desc_dp_top}}px {{bs.repeat_desc_dp_right}}px {{bs.repeat_desc_dp_bottom}}px {{bs.repeat_desc_dp_left}}px;"
            >{{bs.repeat_sec_content}}</p>
          </div>
        </div>
        <div class="recycle-Packaging-repeating-content recycle-Packaging-repeating-mobile flex" data-section-id="{{ block.id }}"
          style="margin:{{bs.repeat_sec_m_top}}px {{bs.repeat_sec_m_right}}px {{bs.repeat_sec_m_bottom}}px {{bs.repeat_sec_m_left}}px;"
        >
          <div class="recycle-Packaging-repeating-desc flex">
            <div class="recycle-content-header">
              <div class="recycle-Packaging-circle">
                <span>{{ forloop.index }}</span>
              </div>
              <h1
                style="color:{{bs.repeat_title_color}};font-size:{{bs.repeat_heading_font_size}}px;font-weight:{{bs.repeat_heading_font_weight}};text-align:{{bs.repeat_heading_alignment}};
                padding:{{bs.repeat_heading_top}}px {{bs.repeat_heading_right}}px {{bs.repeat_heading_bottom}}px {{bs.repeat_heading_left}}px;"
              >{{bs.repeat_sec_title}}</h1>
            </div>
            <p
              style="color:{{bs.repeat_desc_color}};font-size:{{bs.repeat_desc_font_size}}px;font-weight:{{bs.repeat_desc_font_weight}};text-align:{{bs.repeat_desc_alignment}};
              --pd-desc-desktop:{{bs.repeat_desc_dp_top}}px {{bs.repeat_desc_dp_right}}px {{bs.repeat_desc_dp_bottom}}px {{bs.repeat_desc_dp_left}}px; --pd-desc-mobile:{{bs.repeat_desc_dp_top_mb}}px {{bs.repeat_desc_dp_right_mb}}px {{bs.repeat_desc_dp_bottom_mb}}px {{bs.repeat_desc_dp_left_mb}}px;"
            >{{bs.repeat_sec_content}}</p>
          </div>
        </div>
      {% endfor %}
    </div>
    <div class="recycle-Packaging-img">
      <img src="{{ ss.image | img_url: 'master' }}"  alt="{{ ss.image.alt | escape }}"  class="cust_pdp_img bg-cover bg-no-repeat bg-center" /> 
    </div>
  </div>
</div>

<!-- Recycle & Packaging End -->
<style>
.recycle-Packaging-main{
     display:flex;
    width:100%;
        justify-content: flex-start;
    align-items: flex-start;
}
.recycle-Packaging-content{
     width:50%;
    display:flex;
    flex-direction:column;
}
.recycle-Packaging-img{
    padding-left:60px;
}
.recycle-Packaging-Heading h1{
    margin:0px;
}

.recycle-Packaging-repeating-content{
    justify-content:flex-start ;
    align-items:flex-start;
    gap:20px;
}
.recycle-Packaging-circle{
    display:flex;
    justify-content:center;
    align-items:center;
    width: 42px;
    height: 42px;
    background-color: #e08a73;
    border-radius:50%;
    padding: 3.4px 13.8px 6.2px 14.5px;

}
.recycle-Packaging-circle span{
  text-align: center;
}
.recycle-Packaging-repeating-desc{
    flex-direction:column;
}
.recycle-Packaging-repeating-desc h1,p{
    line-height:normal;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: normal;
}
.recycle-Packaging-img{
    width:50%;
    display:flex;
}
.recycle-Packaging-img img{
        width:100%;
        height:auto;
}
.recycle-content-header{
    display: flex;
    justify-content: flex-start;
    align-items: center;
    gap: 16px;
    margin-bottom: 16px;
}
.recycle-content-header h1{
    letter-spacing: normal;
    text-align: left;
    color: #254a23;
}
.recycle-Packaging-repeating-mobile{
    display:none;
}
.recycle-Packaging-repeating-desc p{
    padding: var(--pd-desc-desktop);
}

 @media (max-width: 767px) {
    .recycle-Packaging-main{
        padding:64px 16px !important;
        flex-direction:column-reverse;
    }
    .recycle-Packaging-content{
     width:100%;
    }

    .recycle-content-header{
        margin-bottom: 0;
    }
    
    .recycle-Packaging-img{
        width:100%;
        padding:24px 0px;
    }
    .recycle-Packaging-img img{
        width:100%;
        height:auto;
    }
    .recycle-Packaging-Heading h1{
        font-style: normal;
        line-height: 0.97;
        letter-spacing: -0.8px;
        text-align: left;
        color: {{block.settings.main_title_color}};
    }
    .recycle-Packaging-repeating-desktop{
        display:none;
    }
    .recycle-Packaging-repeating-mobile{
        display:block;
    }
    .recycle-Packaging-repeating-desc h1{
        color: {{block.settings.repeat_title_color}} !important;
    }
    .recycle-Packaging-repeating-desc p{
        letter-spacing: normal;
        text-align: left;
        color: {{block.settings.repeat_desc_color}} !important;
        padding: var(--pd-desc-mobile);
    }
 }
</style>
