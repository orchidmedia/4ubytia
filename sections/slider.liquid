{%- assign st = section.settings -%}
{%- if section.settings.slideshow_height == 'adapt' -%}
  {%- assign first_block = section.blocks[0] -%}
  {%- if first_block.settings.background.aspect_ratio == blank -%}
    {%- assign min_aspect_ratio = 2.333 -%}
  {%- else -%}
    {%- assign min_aspect_ratio = first_block.settings.background.aspect_ratio -%}
  {%- endif -%}
  {% assign wrapper_height = 100 | divided_by: min_aspect_ratio %}
{%- endif -%}
{% case section.settings.bg_color_selector %}
  {% when 'color_section1_bg' %}
    {%- assign bg_color = settings.color_section1_bg -%}
  {% when 'color_section2_bg' %}
    {%- assign bg_color = settings.color_section2_bg -%}
  {% when 'color_section3_bg' %}
    {%- assign bg_color = settings.color_section3_bg -%}
  {% when 'color_section4_bg' %}
    {%- assign bg_color = settings.color_section4_bg -%}
  {% when 'color_section5_bg' %}
    {%- assign bg_color = settings.color_section5_bg -%}
  {% when 'color_section6_bg' %}
    {%- assign bg_color = settings.color_section6_bg -%}
  {% else %}
    {%- assign bg_color = settings.color_section1_bg -%}
{% endcase %}
{%- if section.settings.bg_color_override -%}
  {%- assign bg_color = section.settings.background_color -%}
{%- endif -%}

<style>
    [id^=sf__slideshow-] .sf__dot:before {
        background-color: {{ st.dots_color }};
    }
    [id^=sf__slideshow-] .sf__dot svg circle {
        stroke: {{ st.dots_color }};
    }
    {% if st.no_margin %}
        #sf__slideshow-{{ section.id }} .sf-slider__controls--absolute button {
            margin-top: 0;
        }
    {% endif %}

    #sf__slideshow-{{ section.id }} .sf-slider__controls--absolute button {
        opacity: 1;

        {% if st.nav_position == "top" %}
            top: 12% !important;
        {% elsif st.nav_position == "middle" %}
            top: 50% !important;
        {% elsif st.nav_position == "bottom" %}
            top: 94% !important;
        {% endif %}  
    }
</style>

<section
    id="sf__slideshow-{{ section.id }}"
    class="relative {{ section.settings.custom_class }} slider-{{ section.id }} home-banner__slideshow {% if section.settings.dots_position != 'bottom' %}sf-slider__pagination--{{ section.settings.dots_position }}{% endif %} section-pv-{{ section.settings.margin_vertical }} {% if st.hide_mobile %} hide-mobile{% endif %}{% if st.hide_tablet %} hide-tablet{% endif %}{% if st.hide_desktop %} hide-desktop{% endif %}"
    data-section-type="slider" data-section-id="{{ section.id }}"
    data-autoplay="{% if request.design_mode %}false{% else %}{{ st.autorotate }}{% endif %}"
    data-autoplay-speed="{{ st.autorotate_speed }}"
    data-enable-arrows="{{ st.show_arrows }}"
    data-enable-dots="{{ st.show_dots }}"
    data-enable-loop="{{ st.enable_loop }}"
    data-slide-height="{{ section.settings.slideshow_height }}"
    {% if section.settings.slideshow_height == 'adapt' %}
        data-min-aspect-ratio="{{ min_aspect_ratio }}"
    {% endif %}
    style="--data-autoplay-speed:{{ st.autorotate_speed }}s;background-color:{{ bg_color }};"
>
    <div class="{{ section.settings.container }}">
        <div id="sf-slider-{{ section.id }}" class="swiper-container">
            <div
                {% if section.settings.video_aspect_ratio == "" %}
                    class="swiper-wrapper sf__slider sf__slider--{{ st.slideshow_height }}"
                    {% if section.settings.slideshow_height == 'adapt' %}
                        data-min-aspect-ratio="{{ min_aspect_ratio }}" 
                        style="--slide-height: {{- wrapper_height -}}vw;" 
                    {% endif %} 
                {% else %}
                    class="swiper-wrapper sf__slider"
                    style="aspect-ratio: {{- section.settings.video_aspect_ratio -}};"
                {% endif %}
            >
                {%- for block in section.blocks %}
                    {%- liquid
                        assign bs = block.settings
                        assign image_position = bs.image_alignment
                        assign content_positions = bs.content_position | split: ' '
                        assign text_alignment = bs.text_alignment
                        assign text_horizontal_alignment = content_positions.first
                        assign text_vertical_alignment = content_positions.last | strip
                        assign footer_alignment = bs.footer_alignment
                        assign content_container = 'container-fluid'
                        if bs.content_in_container == true
                            assign content_container = 'container'
                        endif
                        assign bg_image = bs.background
                        if bs.mb_background != blank
                            assign bg_image = bs.mb_background
                        endif 
                    -%}

                    {% capture lightbox_container %}
                        {%- if bs.lightbox_enabled -%}
                            <div data-index="{{ forloop.index0 }}" data-open-lightbox 
                                class="py-2"
                                style="--slider-video-play-color:{{ bs.lightbox_btn_color }};--slider-video-play-bg-color:{{ bs.lightbox_btn_bg_color }};--btn-color:{{ bs.lightbox_btn_color}};max-width:fit-content;"
                            >
                                {%- if bs.lightbox_default_btn -%}
                                    {% render 'button-link'
                                        style: bs.lightbox_btn_style,
                                        text: bs.lightbox_btn_label,
                                        width: bs.lightbox_btn_width,
                                        override_width: bs.lightbox_btn_override_width,
                                        classes: bs.lightbox_btn_classes
                                    %}
                                {%- else -%}
                                    {%- assign button_class = "" -%}
                                    {%- case bs.lightbox_btn -%}
                                        {%- when 'outline' -%}
                                            {%- assign button_class = "slider-play__outline" -%}
                                        {%- when 'solid' -%}
                                            {%- assign button_class = "slider-play__solid" -%}
                                    {%- endcase -%}
                                    <div class="slider-video__play {{ button_class }} {{ bs.lightbox_btn_size }}">
                                        {% render 'icon', name: 'play' %}
                                    </div>
                                {%- endif -%}
                            </div>
                        {%- endif -%}
                    {% endcapture %}

                    {% capture video_bg %}
                        {% if bs.video_selector == 'video-format' and bs.video_controls %}
                            {%- liquid
                                assign controls_position = bs.controls_position | split: ' '
                                assign controls_horizontal_alignment = controls_position.first
                                assign controls_vertical_alignment = controls_position.last | strip
                            -%}
                            <div class="sf__video-controls sf__video-controls--{{ controls_vertical_alignment }}-{{ controls_horizontal_alignment }}">
                                <button class="sf__video-controls-pause" aria-label="play / pause video">
                                    <svg width="42" height="42" viewBox="0 0 42 42" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M42 21C42 32.598 32.598 42 21 42C9.40202 42 0 32.598 0 21C0 9.40202 9.40202 0 21 0C32.598 0 42 9.40202 42 21ZM13 12C13 11.4477 13.4477 11 14 11H18C18.5523 11 19 11.4477 19 12V29C19 29.5523 18.5523 30 18 30H14C13.4477 30 13 29.5523 13 29V12ZM25 11C24.4477 11 24 11.4477 24 12V29C24 29.5523 24.4477 30 25 30H29C29.5523 30 30 29.5523 30 29V12C30 11.4477 29.5523 11 29 11H25Z" fill="white"/>
                                    </svg>                                        
                                </button>
                                {%- if bs.lightbox_enabled and bs.video_controls -%}
                                <button data-index="{{ forloop.index0 }}" data-open-lightbox aria-label="watch video fullscreen">
                                    <svg width="42" height="42" viewBox="0 0 42 42" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M42 21C42 32.598 32.598 42 21 42C9.40202 42 0 32.598 0 21C0 9.40202 9.40202 0 21 0C32.598 0 42 9.40202 42 21ZM10 18.1016V10H13V10L18.1017 10V13L13 13V18.1016H10ZM23.7375 10L31.8391 10V10H31.8393V18.1016H28.8393V13L23.7375 13V10ZM31.8399 23.7375L31.8399 31.8391L31.8398 31.8391H23.7382V28.8391H28.8399L28.8399 23.7375H31.8399ZM12.9998 31.8391H9.99992H9.99984V23.7375H12.9998V28.8391L18.1015 28.8391V31.8391L12.9998 31.8391Z" fill="white"/>
                                    </svg>  
                                </button>                                          
                                {%- endif -%}
                            </div> 
                        {% endif %}
                        <div class="{% if bs.video_selector == 'video-format' %}sf__slide-bg{% endif %} sf-slider-media-item {% if block.settings.show_overlay %}sf-hero--has-overlay{% endif %} {% if bs.video_selector == 'youtube-vimeo' %}sf__slide-iframe{% endif %}">
                            {% if bs.video_selector == 'video-format' %}
                                {% liquid 
                                    assign video_url = bs.video | split: '?v='
                                    assign video_url = video_url | first | strip_newlines
                                    assign video_captions = bs.video_captions
                                    assign format = ''
                                    if video_url contains '.mp4'
                                        assign format = 'mp4'
                                    endif
                                    if video_url contains '.webm'
                                        assign format = 'webm'
                                    endif
                                    if video_url contains '.mov'
                                        assign format = 'mp4'
                                    endif
                                %}
                                <video loop playsinline muted autoplay preload="auto" {% if bs.poster_img != blank %}poster="{{bs.poster_img | img_url: '1920x'}}"{% endif %}>
                                    <source src="{{ video_url }}" type="video/{{ format }}">
                                    {% if video_captions %}<track label="English" kind="subtitles" srclang="en" src="{{video_captions}}" default>{% endif %}
                                    Your browser does not support the video tag.
                                </video>
                                {% if bs.poster_img != blank %}
                                    <link rel="preload" as="image" href="{{bs.poster_img | img_url: '1920x'}}" fetchpriority='high' />
                                {% endif %}
                            {% elsif bs.video_selector == 'youtube-vimeo' %}
                                {% if bs.video_url contains 'youtube.com/watch' %}
                                    {%- liquid
                                        assign video_id = bs.video_url | split: 'v='
                                        assign video_id = video_id[1] | split: '&' | first
                                        assign controls = bs.video_controls
                                    -%}
                                    <div class="plyr__video-embed sf__slide-youtube">
                                        <iframe
                                            src="https://www.youtube.com/embed/{{ video_id }}?origin=https://plyr.io&amp;playlist={{ video_id }}&amp;iv_load_policy=3&amp;modestbranding=1&amp;playsinline=1&amp;showinfo=0&amp;rel=0&amp;enablejsapi=1&autoplay=1&controls={% if controls %}1{% else %}0{% endif %}&loop=1&mute=1&cc_load_policy=1&hl=en&cc_lang_pref=en"
                                            allowfullscreen allowtransparency allow="autoplay" allow=autoplay>
                                        </iframe>
                                    </div>
                                {% elsif bs.video_url contains 'youtu.be/' %}
                                    {%- liquid 
                                        assign video_id = bs.video_url | split: '.be/'
                                        assign video_id = video_id[1] | split: '&' | first 
                                        assign controls = bs.video_controls
                                    -%}
                                    <div class="plyr__video-embed sf__slide-youtube">
                                        <iframe
                                            src="https://www.youtube.com/embed/{{ video_id }}?origin=https://plyr.io&amp;playlist={{ video_id }}&amp;iv_load_policy=3&amp;modestbranding=1&amp;playsinline=1&amp;showinfo=0&amp;rel=0&amp;enablejsapi=1&autoplay=1&controls={% if controls %}1{% else %}0{% endif %}&loop=1&mute=1&cc_load_policy=1&hl=en&cc_lang_pref=en"
                                            allowfullscreen allowtransparency allow="autoplay">
                                        </iframe>
                                    </div>  
                                {% elsif bs.video_url contains 'vimeo.com' %}
                                    {%- liquid
                                        assign video_id = bs.video_url | split: '.com/'
                                        assign video_id = video_id[1] | split: '/' | first
                                        assign video_captions = bs.video_captions_vimeo
                                        assign controls = bs.video_controls
                                    -%}
                                    <div class="plyr__video-embed sf__slide-vimeo"> 
                                        <iframe 
                                            src="https://player.vimeo.com/video/{{ video_id }}?h=69768d2afa{% if video_cations == 'custom' %}&texttrack=en{% elsif video_captions == 'generated' %}&texttrack=en-x-autogenerated{% endif%}&autoplay=1&loop=1&muted=1&autopause=0&controls={% if controls %}1{% else %}0{% endif %}"
                                            frameborder="0" allowfullscreen allowtransparency allow="autoplay">
                                        </iframe>
                                    </div>
                                {% endif %}
                            {% endif %}
                        </div>
                    {% endcapture %}
                    
                    <div
                        {{ block.shopify_attributes }}
                        data-slide="{{ forloop.index0 }}"
                        class="swiper-slide relative {% if forloop.first %} sf-slider--first{% endif %}"
                        data-slide-type="{{ block.type }}"
                        {% if section.settings.video_aspect_ratio %}
                        style="aspect-ratio: {{- section.settings.video_aspect_ratio -}};"
                        {% endif %}
                    >
                        <div class="hidden md:block h-full bg-cover bg-center {% if block.settings.show_overlay %}sf-hero--has-overlay{% endif %}">
                            {% if bs.background == blank and bs.video == blank %}
                                <div class="sf__slide-image sf__slideshow-placeholder absolute inset-0 overflow-hidden">
                                    {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                                    {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'sf__placeholder-svg w-full h-full' }}
                                </div>
                            {% else %}
                                {% if block.type == 'video_slide' %}
                                    {%- if bs.lightbox_enabled -%}
                                        <div data-glightbox
                                            data-source-url="{{ bs.lightbox_video_url }}"
                                            data-media-type="video"
                                        ></div>
                                    {%- endif -%}
                                    {{ video_bg }}
                                {% else %}
                                    {%- if bs.lightbox_enabled -%}
                                        <div data-glightbox
                                            data-src="{{ bs.background | img_url: '1080x' }}"
                                            data-media-type="image"
                                        ></div>
                                    {%- endif -%}
                                    {% if bs.section_preload %}
                                    <div
                                        class="sf__slide-bg sf-slider-media-item bg-cover bg-no-repeat bg-center js"
                                        style="background-image: url('{{ bs.background | img_url: 'original' }}'); background-position: {{ image_position | default: 'center' }};"
                                    >
                                    </div>
                                    <link rel="preload" as="image" href="{{ bs.background | img_url: 'original' }}" fetchpriority='high' />                
                                    {% else %}
                                    <div
                                        class="sf__slide-bg sf-slider-media-item bg-cover bg-no-repeat bg-center lazyload{% unless forloop.first %} lazypreload{% endunless %} js"
                                        data-bg="{{ bs.background | img_url: '300x300' }}"
                                        data-bgset="{% render 'bgset', image: bs.background %}"
                                        data-parent-fit="contain"
                                        data-sizes="auto"
                                        style="background-position: {{ image_position | default: 'center' }};"
                                        data-image-loading>
                                    </div>
                                {% endif %}
                                {% endif %}
                            {% endif %}
                            {% if st.show_overlay %}
                                <div class="sf__slideshow-overlay"></div>
                            {% endif %}
                            <div class="sf__slide-block slide__block-desktop flex sf__slide-block--{{ text_vertical_alignment }}-{{ text_horizontal_alignment }} {{ content_container }} w-full py-24 z-10 {% if bs.video_selector == 'youtube-vimeo' %}absolute {% else %}h-full relative{% endif %}">
                                <div class="sf__slide-content max-w-4xl w-max text-{{ text_alignment }} {% if bs.blurry_bg_enabled %}{{ bs.blurry_bg_desktop }}{% endif %}">
                                    {% if bs.subheading != blank %}
                                        {% capture custom_classes %}slide__block-subtitle mb-[14px]{% endcapture %}
                                        {%- capture custom_styles -%}
                                            {%- if bs.subheading_color !='rgba(0,0,0,0)' and bs.subheading_color != blank -%}
                                                color:{{ bs.subheading_color }};
                                            {%- endif -%}
                                            padding: {{ bs.subheading_padding }}px 0;
                                            display: block;
                                        {%- endcapture -%}

                                        {% render 'heading-tag' 
                                            heading: bs.subheading_tag,
                                            content: bs.subheading,
                                            custom_classes: custom_classes,
                                            custom_styles: custom_styles
                                        %}
                                    {% endif %}
                                    {% if bs.title != blank %}
                                        {% capture custom_classes %}slide__block-title{% endcapture %}
                                        {%- capture custom_styles -%}
                                            {%- if bs.heading_color !='rgba(0,0,0,0)' and bs.heading_color != blank -%}
                                                color:{{ bs.heading_color }};
                                            {%- endif -%}
                                            padding: {{ bs.heading_padding }}px 0;
                                            display: block;
                                        {%- endcapture -%}

                                        {% render 'heading-tag' 
                                            heading: bs.title_tag,
                                            content: bs.title,
                                            custom_classes: custom_classes,
                                            custom_styles: custom_styles
                                        %}
                                    {% endif %}
                                    {% if bs.description != blank %}
                                        {% capture custom_classes %}slide__block-description {% endcapture %}
                                        {%- capture custom_styles -%}
                                            {%- if bs.description_color !='rgba(0,0,0,0)' and bs.description_color != blank -%}color:{{ bs.description_color }};{%- endif -%}
                                            padding: {{ bs.text_padding}}px 0;
                                            display: block;
                                        {%- endcapture -%}

                                        {% render 'heading-tag' 
                                            heading: bs.description_tag,
                                            content: bs.description,
                                            custom_classes: custom_classes,
                                            custom_styles: custom_styles
                                        %}
                                    {% endif %}
                                    {% if bs.button_label != blank %}
                                        {%- case text_alignment -%}
                                            {%- when 'left' -%}
                                                {%- assign btn_pos = 'start' -%}
                                            {%- when 'center' -%}
                                                {%- assign btn_pos = 'center' -%}
                                            {%- when 'right' -%}
                                                {%- assign btn_pos = 'end' -%}
                                        {%- endcase -%}
                                        <div class="sf-custom__text-cta gap-{{ bs.buttons_gap }} flex flex-wrap justify-{{ btn_pos }}">
                                            {% render 'button-link' 
                                                link: bs.button_link,
                                                style: bs.button_style,
                                                text: bs.button_label,
                                                width: bs.button_width,
                                                override_width: bs.override_width
                                            %}
                                            {% if bs.show_second_button and bs.button_label_2 != blank %}
                                                {% render 'button-link' 
                                                    link: bs.button_link_2,
                                                    style: bs.button_style_2,
                                                    text: bs.button_label_2,
                                                    width: bs.button_width_2,
                                                    override_width: bs.override_width_2
                                                %}
                                            {% endif %}
                                        </div>
                                    {% endif %}
                                    
                                    {{ lightbox_container }}
                                </div>
                            </div>
                            {% if bs.show_footer %}
                                <div class="sf__slide-footer {{ content_container }} absolute inset-x-0 bottom-16 z-10 flex items-center justify-{{ footer_alignment }}">
                                    <span class="mx-2">{{ bs.footer_text }}</span>
                                    {% if bs.footer_link != blank %}
                                        <span>|</span>
                                        <a
                                          href="{{ bs.footer_link | default: "#" }}"
                                          class="ml-2 sf__btn sf__btn-link">
                                            {{ bs.footer_button }}
                                        </a>
                                    {% endif %}
                                </div>
                            {% endif %}
                        </div>

                        <div class="md:hidden block pb-6 bg-cover bg-center {%if block.type == 'video_slide' %}sm:h-full{% endif %} {% if block.settings.show_overlay %}sf-hero--has-overlay{% endif %}">
                            {% if bg_image == blank and bs.video == blank %}
                                {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                                <div class="sf__slide-image overflow-hidden">{{ 'lifestyle-' | append: current | placeholder_svg_tag: 'sf__placeholder-svg w-full h-full' }}</div>
                            {% else %}
                                {% if block.type == 'video_slide' and bs.mb_background == blank %}
                                    {%- if bs.lightbox_enabled -%}
                                        <div data-glightbox
                                            data-source-url="{{ bs.lightbox_video_url }}"
                                            data-media-type="video"
                                        ></div>
                                    {%- endif -%}
                                    {{ video_bg }}
                                {% else %}
                                    {%- if bs.lightbox_enabled -%}
                                        <div data-glightbox
                                            data-src="{{ bs.mb_background | img_url: '1080x' }}"
                                            data-media-type="image"
                                        ></div>
                                    {%- endif -%}
                                    <div class="sf__slide-image sf-slider-media-item relative">
                                        {% render 'native-responsive-image',
                                            image: bg_image,
                                            image_class: '',
                                            preload: bs.section_preload %}
                                    </div>
                                {% endif %}
                            {% endif %}
                            {% if bs.subheading != blank or bs.title != blank or bs.lightbox_enabled or bs.description != blank or bs.button_label != blank %}
                                {% liquid
                                    assign text_over = false
                                    if block.type == "video_slide"
                                        assign text_over = true
                                    endif
                                %}
                                {% liquid
                                    assign si_classes = ''
                                    if block.type == 'slider_item'
                                        assign si_classes = 'transform-none-important relative-important mt-7'
                                    endif
                                %}
                                <div class="sf__slide-block flex  sf__slide-block--{{ text_vertical_alignment }}-{{ text_horizontal_alignment }} container-fluid w-full h-full py-6 px-6 pb-0 relative z-10 {{ si_classes }}">
                                    <div class="sf__slide-content sf__slide-content--mobile w-full text-{{ text_alignment }} px-4 {% if bs.blurry_bg_enabled %}{{ bs.blurry_bg_mobile }}{% endif %}">
                                        {% if bs.subheading != blank %}
                                            {% capture custom_classes %}sf__mobile-subheading{% endcapture %}
                                            {%- capture custom_styles -%}
                                                {%- if text_over -%}color:{{ bs.subheading_color }};{%- endif -%}
                                                padding: {{ bs.subheading_padding }}px 0;
                                                display: block;
                                            {%- endcapture -%}

                                            {% render 'heading-tag' 
                                                heading: bs.subheading_tag,
                                                content: bs.subheading,
                                                custom_classes: custom_classes,
                                                custom_styles: custom_styles
                                            %}
                                        {% endif %}
                                        
                                        {{ lightbox_container }}

                                        {% if bs.title != blank %}
                                            {% capture custom_classes %}sf__mobile-heading{% endcapture %}
                                            {%- capture custom_styles -%}
                                                {%- if text_over -%}color:{{ bs.heading_color }};{%- endif -%}
                                                padding: {{ bs.heading_padding }}px 0;
                                                display: block;
                                            {%- endcapture -%}

                                            {% render 'heading-tag' 
                                                heading: bs.title_tag,
                                                content: bs.title,
                                                custom_classes: custom_classes,
                                                custom_styles: custom_styles
                                            %}
                                        {% endif %}
                                        {% if bs.description != blank %}
                                            {% capture custom_classes %}sf__mobile-description text-color-subtext rte mb-5{% endcapture %}
                                            {%- capture custom_styles -%}
                                                {%- if text_over -%}color:{{ bs.description_color }};{%- endif -%}
                                                padding: {{ bs.text_padding }}px 0;
                                                display: block;
                                            {%- endcapture -%}

                                            {% render 'heading-tag' 
                                                heading: bs.description_tag,
                                                content: bs.description,
                                                custom_classes: custom_classes,
                                                custom_styles: custom_styles
                                            %}
                                        {% endif %}
                                        {% if bs.button_label != blank %}
                                            {%- case text_alignment -%}
                                                {%- when 'left' -%}
                                                    {%- assign btn_pos = 'start' -%}
                                                {%- when 'center' -%}
                                                    {%- assign btn_pos = 'center' -%}
                                                {%- when 'right' -%}
                                                    {%- assign btn_pos = 'end' -%}
                                            {%- endcase -%}
                                            <div class="sf-custom__text-cta mt-8 gap-{{ bs.buttons_gap }} flex flex-wrap justify-{{ btn_pos }}">
                                                {% render 'button-link' 
                                                    link: bs.button_link,
                                                    style: bs.button_style,
                                                    text: bs.button_label,
                                                    width: bs.button_width,
                                                    override_width: bs.override_width
                                                %}
                                                {% if bs.show_second_button and bs.button_label_2 != blank %}
                                                    {% render 'button-link' 
                                                        link: bs.button_link_2,
                                                        style: bs.button_style_2,
                                                        text: bs.button_label_2,
                                                        width: bs.button_width_2,
                                                        override_width: bs.override_width_2
                                                    %}
                                                {% endif %}
                                            </div>
                                        {% endif %}
                                    </div>
                                </div>
                            {% endif %}

                            {% if bs.show_footer %}
                                <div class="sf__slide-footer px-3 mt-2 flex justify-center opacity-70">
                                    <div class="px-3">
                                        <span class="mx-2 pb-1">
                                            {{ bs.footer_text }}
                                        </span>
                                        <span>|</span>
                                        <a
                                            href="{{ bs.footer_link | default: "#" }}"
